<div class="loginBox">
  <h3 class="h3"> Log In </h3>
  <%= form_for(:session, url: login_path, remote: true) do |f| %>
    <%= f.email_field :email, class: "emailBox", placeholder: "email/username" %>
    <%= f.password_field :password, class:"emailBox", placeholder:"password" %>
    <%= link_to "(forgot password)", new_password_reset_path %>
    <%= f.submit "Log in", class:"loginButton" %>
    <%= f.label :remember_me do %>
      <%= f.check_box :remember_me %>
      <span>Remember me on this computer</span>
    <% end %>
  <% end %>
  <p onclick="dooo()"> New User? </p>
</div>
